        <activity
            android:name=".GalleryActivity"
            android:label="@string/title_activity_gallery" >
        </activity>
        <activity
            android:name=".PrefrencesActivity"
            android:label="@string/title_activity_prefrences" >
        </activity>
    </application>

</manifest>
